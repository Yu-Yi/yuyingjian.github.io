    <span class="p">{</span>
      <span class="nx">mul</span> <span class="o">=</span> <span class="nx">smalltestCommonsMultiple</span><span class="p">(</span><span class="nx">mul</span><span class="p">,</span><span class="nx">i</span><span class="p">);</span>
    <span class="p">}</span>
  <span class="k">return</span> <span class="nx">mul</span><span class="p">;</span>
<span class="p">}</span>

