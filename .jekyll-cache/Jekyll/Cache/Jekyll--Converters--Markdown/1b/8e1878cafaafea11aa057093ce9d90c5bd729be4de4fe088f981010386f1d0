  <span class="c1">//é€’å½’</span>
  <span class="k">if</span><span class="p">(</span><span class="nx">b</span><span class="p">)</span> <span class="k">return</span> <span class="nx">greatestCommonsDivisor</span><span class="p">(</span><span class="nx">b</span><span class="p">,</span><span class="nx">a</span><span class="o">%</span><span class="nx">b</span><span class="p">);</span>
